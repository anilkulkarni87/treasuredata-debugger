{
  "manifest_version": 3,
  "name": "Treasure Data Debugger (MV3)",
  "version": "3.2",
  "description": "DevTools panel to inspect Treasure Data browser SDK requests & payloads (generic, pluggable).",
  "minimum_chrome_version": "114",
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "128": "icons/icon128.png"
  },
  "devtools_page": "devtools.html",
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "permissions": [
    "storage"
  ],
  "host_permissions": [
    "https://*.treasuredata.com/*",
    "https://*.in.treasuredata.com/*"
  ],
  "action": {
    "default_title": "Treasure Data Debugger"
  },
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self'"
  }
}